     \n            <Col1>\n                <Name>{user.name}</Name>\n                {user.avatar_url ? <Avatar src={user.avatar_url} alt={user.name}/>:<Avatar src={userAvatar} alt={user.name}/>}\n                <StyledLink onClick={() => history.push('/edit')}>Editar Perfil</StyledLink>\n                <Bio>{user.bio}</Bio>\n                <Tecnologias>\n                    <Titles>Tecnologias</Titles>\n                    { visibleTech && <AddTech setVisibleTech={setVisibleTech}/>}\n                    {user.techs && user.techs.map((tech) =>{\n                        return(\n                            <Tecnologia>\n                                 <h5>{tech.title}</h5>\n                                 {tech.status === 'Avançado' ? \n                                 <Bar style={{width:\"240px\"}}></Bar> : \n                                 tech.status === 'Intermediário' ? \n                                 <Bar style={{width:\"180px\"}}></Bar> : \n                                 <Bar style={{width:\"100px\"}}></Bar>}\n                            </Tecnologia>\n                           \n\n                        )\n                    })}\n                    <Button onClick={() => setVisibleTech(true)}>Adicionar Tecnologia</Button>\n                </Tecnologias>\n                \n            </Col1>\n            <Col2>\n                <WorkHeader>\n                    <Titles>Trabalhos</Titles>\n                    <Button onClick={() => setVisibleWork(true)}>Adicionar Trabalho</Button>\n                </WorkHeader>\n               { visibleWork && <AddWork setVisibleWork={setVisibleWork}/>}\n                \n                <Works>\n                    {user.works && user.works.map((work, index) =>{\n                        return(\n                            <Main style={{width: \"500px\"}} key={index}>\n                                <WorkTitle>{work.title}</WorkTitle>\n                                <p>{work.description}</p>\n                                <a target=\"blank\" href={work.deploy_url}>{work.deploy_url}</a>\n                            </Main>\n                            \n                        )\n                    })}\n                    \n                </Works>\n                \n            </Col2>\n            <Col3>\n                <Titles>Dados pessoais</Titles>\n                <Label>Contato</Label>\n                <p>{user.contact}</p>\n                <Label>E-mail</Label>\n                <p>{user.email}</p>\n                <Label>Módulo</Label>\n                <p>{user.course_module}</p>\n            </Col3>\n        </Container>\n        </>\n        \n    )\n\n    \n}\n\nexport default UserProfile","/home/joaomarcos/Documentos/kenziehub/src/store/modules/pageList/actionsTypes.js",[],"/home/joaomarcos/Documentos/kenziehub/src/pages/Login/style.js",[],"/home/joaomarcos/Documentos/kenziehub/src/store/modules/pageList/thunk.js",[],"/home/joaomarcos/Documentos/kenziehub/src/pages/HomeScreen/style/index.js",[],"/home/joaomarcos/Documentos/kenziehub/src/store/modules/pageList/actions.js",[],"/home/joaomarcos/Documentos/kenziehub/src/components/Menu/index.jsx",["155"],"/home/joaomarcos/Documentos/kenziehub/src/components/addTech/index.jsx",[],"/home/joaomarcos/Documentos/kenziehub/src/components/Header/style.js",[],"/home/joaomarcos/Documentos/kenziehub/src/components/Search/index.jsx",[],"/home/joaomarcos/Documentos/kenziehub/src/store/modules/search/actions.js",[],"/home/joaomarcos/Documentos/kenziehub/src/styles.js",[],"/home/joaomarcos/Documentos/kenziehub/src/store/modules/register/reducer.js",[],"/home/joaomarcos/Documentos/kenziehub/src/store/modules/pageList/reducer.js",[],"/home/joaomarcos/Documentos/kenziehub/src/pages/UserList/index.jsx",["156"],"/home/joaomarcos/Documentos/kenziehub/src/store/modules/login/actionType.js",[],"/home/joaomarcos/Documentos/kenziehub/src/services/api.js",[],"/home/joaomarcos/Documentos/kenziehub/src/components/List/style.js",[],"/h