register} />\n        <input name=\"contact\" placeholder=\"Contato\" ref={register} />\n        {password ? (\n          <>\n            <input\n              name=\"old_password\"\n              placeholder=\"Antiga senha\"\n              ref={register}\n            />\n            <input name=\"password\" placeholder=\"Nova senha\" ref={register} />{\" \"}\n          </>\n        ) : null}\n        <button>Teste</button>\n      </form>\n      <p onClick={() => setPassword(!password)}>Alterar senha</p>\n    </div>\n  );\n};\n\nexport default ProfilePage;\n","/home/franciele/kenziehub/src/pages/HomeScreen/index.jsx",[],"/home/franciele/kenziehub/src/components/slide/index.jsx",["121"],"/home/franciele/kenziehub/src/reportWebVitals.js",[],"/home/franciele/kenziehub/src/store/modules/users/thunk.js",[],"/home/franciele/kenziehub/src/components/Carousel/index.jsx",["122","123","124"],"/home/franciele/kenziehub/src/store/modules/register/thunk.js",[],"/home/franciele/kenziehub/src/store/modules/users/actionsTypes.js",[],"/home/franciele/kenziehub/src/pages/Login/style.js",[],{"ruleId":"125","severity":1,"message":"126","line":17,"column":5,"nodeType":"127","messageId":"128","endLine":17,"endColumn":10},{"ruleId":"125","severity":1,"message":"129","line":21,"column":5,"nodeType":"127","messageId":"128","endLine":21,"endColumn":14},{"ruleId":"125","severity":1,"message":"130","line":24,"column":9,"nodeType":"127","messageId":"128","endLine":24,"endColumn":13},{"ruleId":"125","severity":1,"message":"131","line":28,"column":9,"nodeType":"127","messageId":"128","endLine":28,"endColumn":13},{"ruleId":"125","severity":1,"message":"131","line":2,"column":25,"nodeType":"127","messageId":"128","endLine":2,"endColumn":29},{"ruleId":"132","replacedBy":"133"},{"ruleId":"134","replacedBy":"135"},{"ruleId":"136","severity":1,"message":"137","line":40,"column":6,"nodeType":"138","endLine":40,"endColumn":23,"suggestions":"139"},{"ruleId":"140","severity":1,"message":"141","line":53,"co